<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="group03.lab02.PrimaryController">
    <children>
        <TextField fx:id="TFuser" promptText="User ID" />
      <TextField fx:id="TFpass" promptText="Password" />
      <TextArea fx:id="TAtest" prefHeight="57.0" prefWidth="149.0" />
        <HBox prefHeight="0.0" prefWidth="96.0">
            <children>
                <Button mnemonicParsing="false" onAction="#handlelogin" text="Sign In" />
            </children>
        </HBox>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
