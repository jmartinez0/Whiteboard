<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="600.0" prefWidth="960.0" style="-fx-background-color: white;" stylesheets="@styling.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.whiteboard.FacultyViewController">
   <children>
      <HBox prefHeight="60.0" prefWidth="960.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="59.0" prefWidth="235.0" style="-fx-border-color: #0071e3;" styleClass="header">
               <children>
                  <ImageView fx:id="whiteboardLogoImageView" fitHeight="50.0" fitWidth="59.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@WhiteboardLogoWhite.png" />
                     </image>
                  </ImageView>
                  <Label styleClass="header-text" text="  Whiteboard" />
               </children>
               <padding>
                  <Insets left="5.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="61.0" prefWidth="745.0" spacing="40.0" styleClass="header">
               <children>
                  <Label onMouseClicked="#courses" styleClass="header-text" text="Courses" />
                  <Label fx:id="logOutLabel" onMouseClicked="#logOut" styleClass="header-text" text="Log Out" />
               </children>
               <padding>
                  <Insets right="40.0" />
               </padding>
            </HBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
